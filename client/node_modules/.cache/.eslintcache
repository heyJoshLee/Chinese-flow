ownWords)\n\n  if (!knownWords) { return <div>Loading...</div>}\n  console.log(knownWords)\n  return (\n    <div>\n        <h2 className=\"text-center\">Recently Learned Words</h2>\n        <div className=\"jumbotron\">\n          <RecentWordsList words={knownWords} />\n        </div>\n    </div>\n  )\n}\n\nexport default RecentWords;\n","/Users/mymac/repos/chinese-app/client/src/components/resources/New.js",[],"/Users/mymac/repos/chinese-app/client/src/components/resources/Form.js",[],"/Users/mymac/repos/chinese-app/client/src/components/words/New.js",[],"/Users/mymac/repos/chinese-app/client/src/components/words/Form.js",[],"/Users/mymac/repos/chinese-app/client/src/components/resources/Resources.js",["319","320","321","322","323","324","325","326","327"],"import React, { useEffect } from 'react';\nimport ResourcesList from './ResourcesList';\nimport { getResources } from '../../actions/resources';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { getWordsForLoggedInUser } from '../../actions/users';\n\nconst Resources = () => {\n  const dispatch = useDispatch();\n  const resources = useSelector(state => state.resources);\n\n  useEffect(() => {\n    dispatch(getResources());\n    dispatch(getWordsForLoggedInUser());\n  },[])\n\n\n  if (!resources) { \n    dispatch(getResources);\n    return <div> Loading...</div>\n  }\n  return (\n    <div className=\"mt-5\">\n      <h1 className=\"text-center\"> Resources</h1>\n      <div className=\"dropdown show\">\n        <a className=\"btn btn-secondary dropdown-toggle\" href=\"#\" role=\"button\" id=\"dropdownMenuLink\" data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\">\n          Date\n        </a>\n        <div className=\"dropdown-menu\" aria-labelledby=\"dropdownMenuLink\">\n          <a className=\"dropdown-item\" href=\"#\">Newest First</a>\n          <a className=\"dropdown-item\" href=\"#\">Oldest First</a>\n        </div>\n      </div>\n\n<hr />\n\n    \n\n      <ResourcesList