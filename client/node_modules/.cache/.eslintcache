ody text-center\">\n          {/*renderBadge(resource) */}\n          <ProgressBar resource={resource} knownWords={knownWords} />\n          <div className=\"mt-4\"></div>\n          {renderMediumType(resource)}\n          <br/>\n          <small>{resource.words.length} unique words</small>\n\n          <h4 className=\"mt-3 text-center\">{resource.title}</h4>\n          <p className=\"card-text\">{`${resource.description.substring(0, 180)}...`}</p>\n          \n          <Link to={`/resources/${resource._id}`} className=\"btn btn-primary btn-block\">View</Link>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default ResourceItem;\n","/Users/mymac/repos/chinese-app/client/src/components/resources/ResourcesList.js",[],"/Users/mymac/repos/chinese-app/client/src/components/words/RecentWordsList.js",[],"/Users/mymac/repos/chinese-app/client/src/components/words/RecentWordItem.js",[],"/Users/mymac/repos/chinese-app/client/src/components/shared/BackLink.js",[],"/Users/mymac/repos/chinese-app/client/src/actions/resources.js",[],"/Users/mymac/repos/chinese-app/client/src/api/index.js",[],"/Users/mymac/repos/chinese-app/client/src/types/index.js",[],"/Users/mymac/repos/chinese-app/client/src/reducers/index.js",[],"/Users/mymac/repos/chinese-app/client/src/reducers/resource.js",["330"],"import { GET_RESOURCE, UPDATE_RESOURCE, ADD_WORD_TO_RESOURCE, REMOVE_WORD_FROM_RESOURCE } from '../types/index';\n\nexport default (resource = null, action) => {\n  switch (action.type) {\n    case GET_RESOURCE:\n    case UPDATE_RESOURCE:\n    case ADD_WORD_TO_RESOURCE:\n    case REMOVE_WORD_FROM_RESOURCE:\n      return action.payload;\n    default:\n      return resource;\n  }\n}","/Users/mymac/repos/chinese-app/client/src/reducers/resources.js",["331"],"import { GET_RESOURCES,} from '../types/index';\n\nexport default (resources = null, action) => {\n  switch (action.type) {\n    case GET_RESOURCES:\n      return action.payload;\n    default:\n      return resources;\n  }\n}","/Us